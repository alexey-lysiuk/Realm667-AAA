alias menu_r667aaa "openmenu R667AAA_Menu"
addkeysection "Realm667 - An Awesome Awesomeness" R667AAA
addmenukey "R667AAA Menu" menu_r667aaa
defaultbind H menu_r667aaa

weaponsection R667AAA

addslotdefault 1 Axe

addslotdefault 7 BFG2704
addslotdefault 7 STBFG10K
